generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url=env("POSTGRES_URL")
}

model User {
  id  Int @id @default(autoincrement())

  fullName String
  email String
  password String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model example_cards {
  id Int @id @default(autoincrement())
  en_w String
  ru_w String
  transcription String
  listen Json
}